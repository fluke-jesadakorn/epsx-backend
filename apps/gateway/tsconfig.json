{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "outDir": "../../dist/apps/gateway",
    "composite": true,
    "baseUrl": "./",
    "module": "CommonJS",
    "rootDir": "../../",
    "moduleResolution": "Node",
    "paths": {
      "@libs/common": ["../../libs/common"],
      "@libs/common/*": ["../../libs/common/*"]
    }
  },
  "include": ["src/**/*", "../../libs/common/src/**/*"],
  "exclude": ["node_modules", "dist"],
  "incremental": false,
  "declaration": false,
  "tsBuildInfoFile": false,
  "sourceMap": false
}
